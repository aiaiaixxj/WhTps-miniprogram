/* pages/databaseGuide/databaseGuide.wxss */

@import "../../style/guide.wxss";

.guide .counter {
  margin-top: 50rpx;
  display: flex;
  flex-direction: row;
  align-content: space-between;
}
.title_item {
  position: absolute;
  left: 48%;
  overflow: hidden;
  top: 0px;
  width: 400rpx;
  display:-webkit-box;
  -webkit-line-clamp:1;
  overflow:hidden;
  text-overflow:ellipsis;
  -webkit-box-orient:vertical;
  word-break:break-all;
}
.divLine{
  background: #E0E3DA;
  width: 100%;
  height: 3rpx;
 }