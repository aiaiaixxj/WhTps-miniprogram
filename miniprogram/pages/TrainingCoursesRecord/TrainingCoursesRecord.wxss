/* pages/databaseGuide/databaseGuide.wxss */
page {
  width: 100%;
  overflow-x:hidden;
  background-color: #f6f6f6;
}
.user-panel{
  width: 100%;
  overflow-x:hidden;
}
.divLine {
  position: absolute;
  background: #C0C0C0;
  width: 100%;
  height: 2rpx;
  top: 220rpx;
}

.title {
  background-color: #fff;
  color: #000;
  font-weight: bold;
  font-size: 28rpx;
  padding: 20rpx 20rpx 20rpx 40rpx;
  margin-top: 20rpx;
  border-bottom: 2rpx solid #f6f6f6;
  position: relative;
}

.title::after {
  content: "";
  display: block;
  width: 6rpx;
  height: 20rpx;
  background-color: #f40000;
  position: absolute;
  left: 20rpx;
  top: 50%;
  margin-top: -10rpx;
  z-index: 1;
}

.box {
  background-color: #fff;
}

.navigator {
  display: flex;
  align-items: center;
  border-bottom: 2rpx solid #C0C0C0;
  margin-bottom: -10rpx;
  padding-bottom: 33rpx;
}

.head {
  flex-shrink: 0;
  width: 280rpx;
  height: 150rpx;
  margin-right: 20rpx;
  margin-left: 20rpx;
}

.center {
  flex-grow: 1;
  height: 80rpx;
}

.name {
  display: flex;
  align-items: center;
}

.name-text {
  margin-top: -50rpx;
  font-size: 27rpx;
  font-weight: bold;
  color: #333;
  margin-right: 20rpx;
  width: 430rpx;
  display: -webkit-box;
  -webkit-line-clamp: 1;
  overflow: hidden;
  text-overflow: ellipsis;
  -webkit-box-orient: vertical;
  word-break: break-all;
}

.code {
  font-size: 24rpx;
  color: #999;
  margin-top: 27rpx;
}
.icon-class{

  left: -200rpx !important; 
  
}